[color]
        ui = true
[alias]
        co = checkout
        st = status
        ci = commit
        di = diff
        br = branch
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[core]
        excludesfile = ~/.gitignore
        pager = less
[tig "bind"]
        generic = g move-first-line
        generic = G move-last-line
